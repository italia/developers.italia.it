<ul class="pagination pagination-lg">
  <li class="page-item {{ prev.classes }}">
    <a class="page-link" href="{{ prev.url }}" aria-label="Previous" tabindex="-1">
      <span aria-hidden="true">&lt;</span>
      <span class="sr-only">Previous</span>
    </a>
  </li>
{{#each pages }}
  <li class="page-item {{ classes }}">
    <a class="page-link" href="{{ url }}">{{ title }}</a>
  </li>
{{/each}}
  <li class="page-item {{ next.classes }} ">
    <a class="page-link" href="{{ next.url }}" aria-label="Previous" tabindex="-1">
      <span aria-hidden="true">&gt;</span>
      <span class="sr-only">Next</span>
    </a>
  </li>
</ul>